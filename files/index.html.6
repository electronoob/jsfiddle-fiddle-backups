<script type='text/javascript'>//<![CDATA[
window.onload=function(){
var ctx = e.getContext('2d');
var x,y;
x=200;
y=200;

var seed = 313;
function random() {
    var x = Math.sin(seed+=2) * 98900000;
    return x - Math.floor(x);
}
function gri (min, max) {
  min = Math.ceil(min);
  max = Math.floor(max);
  return Math.floor(random() * (max - min + 1)) + min;
}

setInterval(function(){
	var k;
  for(k=0;k<20;k++) {
   // ctx.fillStyle = "rgba(0,0,0,0.1)";
    //ctx.fillRect(0,0,400,400);
    ctx.fillStyle = "rgba("+gri(100,200)+","+gri(100,200)+","+gri(100,200)+",1)";
		var s = 0.03;
    var i;
    for(i=0;i<100/s;i++) {
        switch (gri(0,3)) {
          case 0:
          //up
          y-=s;
          break;
          case 1:
          //down
          y+=s;
          break;
          case 2:
          //left
          x-=s;
          break;
          case 3:
          //right
          x+=s;
          break;
        }
        if ( ( (0>x) || (x>400) ) || ( (0>y) || (y>400) ) )  
        {x = y = 200;}
        ctx.fillRect(x,y,s,s);
      }
     // x=gri(0,400);
     // y=gri(0,400);
  }
}, 1);



}//]]> 

</script>

  
