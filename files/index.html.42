    <div class="achievementIconBox">
      <img class="achievementIcon" src="//slither.io/s/favicon.png">
    </div>
    
    <div class="achievementInfo">
      <div class="achievementName">
        <h3 class="achievementNameTxt">Team Player</h3>
      </div>
      
      <div class="achievementDescription">
        <p class="achievementDescriptionTxt">
          Link a social media account to Slither.io
        </p>
      </div>
    </div>
  </div>
</div>
  
  <script>
  // tell the embed parent frame the height of the content
  if (window.parent && window.parent.parent){
    window.parent.parent.postMessage(["resultsFrame", {
      height: document.body.getBoundingClientRect().height,
      slug: "ay1ueb73"
    }], "*")
  }
</script>

