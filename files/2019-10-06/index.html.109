var messageIndex=0;
function gri(min, max) {
  min = Math.ceil(min);
  max = Math.floor(max);
  return Math.floor(Math.random() * (max - min + 1)) + min;
}
function getRandWord() {
	return words[gri(0,words.length-1)];
}

parseWord("");
function parseWord(word){
  currentWord = word;
  currentWordWidths = [];
  word = word.toUpperCase();
	i = 0;
  buffer = "";
  for(;i<word.length;i++) {
  	ch = word[i];
    f = find.indexOf(ch);
    if (f==-1) return -1;
    r = replace[f];
    buffer += r + "    ";
    ctx.font=size+"px "+font;
    currentWordWidths[i] = ctx.measureText(ch).width - tightness;
  }
  return buffer;
}
function draw(){
  ctx.fillStyle = "rgba(255,255,255,0."+blur+")";
  ctx.fillRect(0,0,700,300);
  ctx.fillStyle = "white";
  //ctx.fillRect(202.5,50, 2, 150);
  ctx.fillStyle = "silver";
	//ctx.fillRect(50,50, 650,7);
  ctx.fillRect(200,50, 2, 20);
	//ctx.fillRect(50,200, 650,7);
  ctx.fillRect(200,200-20+2, 2,20);
  ctx.font=size+"px "+font;
  k = 0;
  characterOffset = 0;
  test = Math.round(currentWord.length/3);
  test2 = 0;
  for(k=0;k<test;k++)
  	test2+=currentWordWidths[k];
  test2 += charpad;
  for(k=0;k<currentWord.length;k++) {
  		if(test == k) {ctx.fillStyle = "red";}else{ctx.fillStyle = "black";}
      ctx.fillText(currentWord[k], characterOffset+200-test2+(size/4), 150);
      characterOffset += currentWordWidths[k];
  }
  window.requestAnimationFrame(draw);
}

window.requestAnimationFrame(draw);
setInterval(function(){
	 currentWordMorse = parseWord( message[messageIndex] );
   messageIndex++;
   if(messageIndex== message.length) messageIndex = 0;
},130);
